<?xml version="1.0" encoding="utf-8"?>
<bkav.android.btalk.messaging.ui.mediapicker.contact.ContactChooserView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
>
    <FrameLayout
        android:id="@+id/contact_picker_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/btalk_white_bg"/>
    <ImageButton
        android:id="@+id/btn_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:padding="10dp"
        android:visibility="visible"
        android:background="@drawable/overfollow_button_bg"
        android:src="@drawable/btalk_search_icon_new_design"/>
</bkav.android.btalk.messaging.ui.mediapicker.contact.ContactChooserView>
